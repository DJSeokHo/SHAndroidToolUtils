<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.swein.framework.module.exceptionreport.ExceptionReportDemoActivity"
    >


    <Button
        android:id="@+id/buttonMakeNormalException"
        android:text="일반 오류 만들기"
        android:layout_gravity="center"
        android:background="#0000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/buttonMakeAPIException"
        android:text="api 호출 주고받기 이상 만들기"
        android:layout_marginTop="50dp"
        android:layout_gravity="center"
        android:background="#0000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</FrameLayout>
